<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.dogdailylog.post.dao.PostMapper">

<select id="selectPostList" resultType="com.dogdailylog.post.model.Post">
	SELECT
		`id`
		, `userId`
		, `content`
		, `imagePath`
		, `createdAt`
		, `updatedAt`
	FROM
		`post`
	ORDER BY `createdAt` DESC
</select>

</mapper>